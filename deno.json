{
  "tasks": {
    "server:dev": "deno task wrangler dev --port 8786 src/main.ts",
    "wrangler": "node node_modules/.bin/wrangler",
    "docio": "deno run --allow-net --allow-write --allow-read --allow-env --allow-run scripts/cli.ts",
    "dev": {
      "command": "echo 'Starting dev server...'",
      "dependencies": ["server:dev"]
    }
  },
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.7",
    "@cloudflare/workers-types": "npm:@cloudflare/workers-types@^4.20250204.0",
    "@david/dax": "jsr:@david/dax@^0.42.0",
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.4.3",
    "@octokit/app": "npm:@octokit/app@^15.1.3",
    "@octokit/core": "npm:@octokit/core@^6.1.4",
    "@std/assert": "jsr:@std/assert@1",
    "@std/http": "jsr:@std/http@1",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/tar": "jsr:@std/tar@^0.1.5",
    "cloudflare": "npm:cloudflare@^4.1.0",
    "hono": "npm:hono@^4.7.1",
    "rspress": "npm:rspress@^1.41.2",
    "wrangler": "npm:wrangler@^3.108.1",
    "zod": "npm:zod@^3.24.2"
  },
  "nodeModulesDir": "auto",
  "workspace": ["./packages/*", "./src"],
  "fmt": {
    "singleQuote": true
  }
}

{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "docio",
  "main": "src/main.ts",
  "compatibility_flags": ["nodejs_compat"],
  "compatibility_date": "2025-02-04",
  "observability": {
    "enabled": true
  },
  "r2_buckets": [
    {
      "bucket_name": "docio-repos",
      "binding": "reposBucket"
    }
  ],
  "tsconfig": "./tsconfig.wrangler.json"
}
